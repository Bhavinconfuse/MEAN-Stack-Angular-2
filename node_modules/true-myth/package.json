  "description": "A library for safe functional programming in JavaScript, with first-class support for TypeScript and Flow",
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "eslint": "^4.19.1",
    "jest": "^22.4.3",
    "prettier": "^1.12.1",
    "prettier-eslint": "^8.8.1",
    "rimraf": "^2.6.2",
    "shelljs": "^0.8.2",
    "ts-jest": "^22.4.6",
    "typedoc": "^0.11.1",
    "typescript": "^2.8.3",
    "typescript-eslint-parser": "^15.0.0"
  },
  "ember-addon": {
    "main": "ember-addon.js"
  },
  "eslintConfig": {
    "parser": "typescript-eslint-parser",
    "extends": [
      "eslint:recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "es6": true
    },
    "rules": {
      "no-undef": "off",
      "no-unused-vars": "off",
      "no-redeclare": "off",
      "no-irregular-whitespace": "off"
    }
  },
  "homepage": "https://true-myth.js.org",
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/*"
    ],
    "coveragePathIgnorePatterns": [
      "src/index.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "statements": 100,
        "lines": 100
      }
    }
  },
  "keywords": [
    "ember-addon",
    "typescript",
    "functional programming",
    "maybe",
    "result"
  ],
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "name": "true-myth",
  "prettier": {
    "printWidth": 100,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "tabWidth": 2
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chriskrycho/true-myth.git"
  },
  "scripts": {
    "build": "yarn clean && yarn ts:all",
    "clean": "rimraf dist && yarn ts:clean",
    "doc": "./scripts/build-docs",
    "lint": "yarn eslint src/*.ts test/*.ts",
    "postpublish": "yarn clean",
    "prepublishOnly": "yarn test && yarn build",
    "preversion": "./scripts/build-docs && git add docs",
    "tdd": "jest --watch",
    "test": "jest && yarn lint",
    "ts:all": "yarn ts:cjs && yarn ts:es && yarn ts:typedefs",
    "ts:cjs": "tsc -p ts/cjs.tsconfig.json",
    "ts:clean": "rimraf ./*.d.ts dist",
    "ts:es": "tsc -p ts/es.tsconfig.json",
    "ts:typedefs": "tsc --outDir . --emitDeclarationOnly"
  },
  "types": "index.d.ts",
  "version": "2.0.0"
}
